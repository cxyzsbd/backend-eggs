{
  "extends": "eslint-config-egg",
  "root": true,
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    // "indent":["off",2],
    // "space-before-function-paren":0,
    "prefer-const": 0,
    "no-unused-vars": "error", // 开启没有用过的变量检测
    "semi": [
      "warn",
      "always"
