{
  "name": "web_backend",
  "version": "1.0.0",
  "description": "backend of cloud-native",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^3",
    "egg-jwt": "^3.1.7",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2",
    "egg-sequelize": "^6.0.0",
    "egg-socket.io": "^4.1.6",
    "egg-swagger-doc-feat": "^2.2.14",
    "egg-validate": "^2.0.2",
    "i18n": "^0.15.1",
    "mysql2": "^2.3.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12",
    "sequelize-automate": "^1.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-web_backend",
    "stop": "egg-scripts stop --title=egg-server-web_backend",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "dbload": "sequelize-automate -c sequelize-automate.config.json",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "ci": {
    "version": "16, 18",
    "type": "github"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "yulinghong",
  "license": "MIT"
}
